    "figures": "3.2.0",
    "flat": "^5.0.2",
    "fs-extra": "^10.1.0",
    "glob": "7.1.4",
    "ignore": "^5.0.4",
    "minimatch": "3.0.5",
    "npm-run-path": "^4.0.1",
    "open": "^8.4.0",
    "rxjs": "^6.5.4",
    "semver": "7.3.4",
    "tmp": "~0.2.1",
    "tslib": "^2.3.0",
    "yargs": "^17.4.0",
    "yargs-parser": "21.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
